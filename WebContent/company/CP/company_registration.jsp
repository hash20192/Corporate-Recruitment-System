	<div id="wrapper">
        <h3>Account Registration</h3>

            <form action="${pageContext.request.contextPath}/Company_Controller" >

                <div id="slider">
                	  <div class="form-step" >
             
             			  <div class="row1">
                            <div class="form-left">Company Name *</div>
                            <div class="form-right"><input type="text" id="company_name" name="company_name" class="form-input" /></div>
                            <div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Company Email *</div>
                            <div class="form-right"><input type="text" id="company_email" name="company_email" class="form-input" /></div>
							<div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Password *</div>
                            <div class="form-right"><input type="text" id="company_password" name="company_password" class="form-input" /></div>
							<div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Confirm Password *</div>
                            <div class="form-right"><input type="text" id="cpass" name="cpass" class="form-input" /></div>
							<div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Company Address *</div>
                            <div class="form-right"><input type="text" id="company_address" name="company_address" class="form-input" /></div>
                            <div class="form-error"></div>
                        </div>
                     
                    </div>
                    
                    
                    <div class="form-step" >

                        <div class="row1">
                            <div class="form-left">First Name *</div>
                            <div class="form-right"><input type="text" id="emr_firstname" name="emr_firstname" class="form-input" /></div>
                            <div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Last Name *</div>
                            <div class="form-right"><input type="text" id="emr_lastname" name="emr_lastname" class="form-input" /></div>
                            <div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Gender *</div>
                            <div class="form-right">
                                <select id="emr_gender" name="emr_gender">
                                    <option value="0">Select</option>
                                    <option value="M">Male</option>
                                    <option value="F">Female</option>
                                </select>
                            </div>
                            <div class="form-error"></div>
                        </div>
                         <div class="row1">
                            <div class="form-left">Nationality *</div>
                            <div class="form-right">
                                <select id="emr_nationality" name="emr_nationality">
                                    <option value="">Select</option>
                                    <option value="indian">Indian</option>
                                    <option value="other">other</option>
                                </select>
                            </div>
                            <div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Address</div>
                            <div class="form-right"><input type="text" id="emr_address" name="emr_address" class="form-input" /></div>
                            <div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Email *</div>
                            <div class="form-right"><input type="text" id="emr_email" name="emr_email" class="form-input" /></div>
<div class="form-error"></div>
                        </div>
                        <div class="row1">
                            <div class="form-left">Mobile No</div>
                            <div class="form-right"><input type="text" id="emr_mobileno" name="emr_mobileno" class="form-input" /></div>
<div class="form-error"></div>
                        </div>
                    </div>
                
                  
                </div>
            </form>
        </div>
	
